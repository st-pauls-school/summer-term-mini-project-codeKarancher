                                </Rectangle.Fill>
                                <Rectangle.Stroke>
                                    <SolidColorBrush Color="#FF90C0FF"/>
                                </Rectangle.Stroke>
                            </Rectangle>
                            <ContentPresenter  RecognizesAccessKey="True"/>
                            <ContentControl HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}" SnapsToDevicePixels="{TemplateBinding SnapsToDevicePixels}" VerticalAlignment="{TemplateBinding VerticalContentAlignment}" Content="Go!" Foreground="GhostWhite" FontSize="10"/>
